{
  "@tailwind components": [
    true,
    true,
    true,
    true
  ],
  "@layer components": [
    {
      ".card": {
        "@apply rounded-lg border border-gray-200 p-5 sm:p-6 lg:p-6": true
      },
      ".card-primary": {
        "@apply card": true,
        "@apply bg-default dark:bg-dark": true
      },
      ".card-primary .card-title": {
        "@apply dark:text-white dark:text-gray-200": true
      }
    },
    {
      ".alert-info": {
        "@apply alert text-blue-700 fill-blue-700": true
      },
      ".alert-warning": {
        "@apply alert text-orange-400  fill-orange-400": true
      },
      ".alert-error": {
        "@apply alert text-red-700  fill-red-700": true
      },
      ".alert-success": {
        "@apply alert text-green-500 fill-green-500": true
      }
    },
    {
      ".list.list-primary": {},
      ".list.list-timeline": {},
      ".list": {
        "@apply p-0": true
      },
      ".list .list-item": {
        "@apply flex gap-1 py-5 align-middle": true
      },
      ".list.list-timeline .list-item": {
        "@apply mx-[23pt] py-0": true
      },
      ".list.list-primary .list-item:hover": {
        "@apply bg-gray-100": true
      },
      ".list.list-primary .list-item.active": {
        "@apply border-r-2 border-red-300": true
      },
      ".list.list-timeline .list-item .list-item-text": {
        "@apply align-middle flex self-center px-3": true
      },
      ".list.list-timeline .list-item .list-item-decoration": {
        "@apply flex flex-col justify-center items-center": true
      },
      ".list.list-timeline .list-item .list-item-dot": {
        "@apply w-2 h-2 bg-gray-300 rounded-full absolute": true
      },
      ".list.list-timeline .list-item .list-item-linedot": {
        "@apply w-[1px] min-h-[70px] h-[100%] bg-gray-300 self-start justify-self-start": true
      },
      ".list.list-timeline .list-item-avatar": {
        "@apply lg:max-w-[50pt] md:max-w-[50pt] max-w-[50px] px-[3pt] mb-0": true
      },
      ".list.list-timeline .list-item-avatar img": {
        "@apply max-w-full max-h-full z-10": true
      },
      ".list .list-item .list-item-icon": {
        "@apply min-w-[10%] flex justify-center text-center align-middle dark:fill-gray-200": true
      },
      ".list .list-item .list-item-text": {
        "@apply dark:text-gray-200": true
      },
      ".list.list-timeline .list-item:last-child .list-item-decoration": {
        "@apply justify-start": true
      },
      ".list.list-timeline .list-item:last-child .list-item-dot": {
        "@apply self-center justify-self-center mt-[20px]": true
      },
      ".list.list-timeline .list-item:last-child .list-item-linedot": {
        "@apply min-h-[20px] max-h-[20px] justify-self-start self-start": true
      },
      ".list.list-timeline .list-item:first-child": {
        "@apply px-0 mx-0": true
      }
    },
    {
      ".swiper-button-next,\n  .swiper-button-prev": {
        "@apply bg-white text-white p-10 rounded-full scale-50 text-[20px] border-[1px]": true
      },
      ".swiper-button-disabled": {
        "@apply hidden invisible": true
      }
    },
    {
      ".title-decoration-underline": {
        "@apply w-16 mt-2 h-[2px] bg-primary": true
      },
      ".text-link": {
        "@apply text-secondary fill-secondary": true
      },
      ".text-warning": {
        "@apply text-orange-400": true
      },
      "a[data-bs-toggle='collapse']": {
        "@apply fill-gray-400 text-gray-400": true
      },
      "a[data-bs-toggle='collapse'][aria-expanded='true'] .open,\n  a[data-bs-toggle='collapse'][aria-expanded='false'] .close": {
        "@apply hidden": true
      }
    }
  ],
  "@tailwind base\n@tailwind components\n\n@layer components": {
    ".alert": {
      "@apply flex p-1 my-1 text-[12px] rounded-lg gap-1 items-center": true
    }
  },
  "@tailwind components\n\n@layer components": [
    {
      ".badge-primary": {
        "@apply rounded bg-gray-400 px-1.5 py-0.5 text-xs font-bold font-semibold text-white dark:bg-gray-700 dark:text-gray-300": true
      },
      ".badge-warning": {
        "@apply rounded-xl bg-orange-100 px-1.5 py-0.5 text-xs font-bold font-semibold text-orange-400 dark:bg-gray-700 dark:text-gray-300": true
      },
      ".active .badge-primary": {
        "@apply bg-primary": true
      }
    },
    {
      ".btn": {
        "@apply text-center justify-center flex rounded-lg bg-primary px-5 py-2.5 text-center text-sm font-medium": true
      },
      ".btn-primary": {
        "@apply btn": true,
        "@apply text-white dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-800": true
      },
      ".btn-success": {
        "@apply btn": true,
        "@apply text-white bg-success dark:bg-success dark:hover:bg-success dark:focus:ring-success": true
      },
      ".btn-primary-outlined": {
        "@apply btn bg-transparent border": true,
        "@apply text-primary border-red-600 dark:hover:bg-red-700 dark:focus:ring-red-800 dark:text-gray-100 dark:bg-transparent": true
      },
      ".btn-success-outlined": {
        "@apply btn-success bg-transparent border": true,
        "@apply text-primary border-success dark:hover:bg-success dark:focus:ring-success dark:text-gray-100 dark:bg-transparent": true
      },
      ".btn-gray-outlined": {
        "@apply flex rounded-lg border border-gray-400 px-5 py-2.5 text-center text-sm font-medium text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-800 dark:text-gray-100 dark:bg-transparent": true
      }
    },
    {
      ".input-primary": {
        "@apply w-full rounded-lg border border-gray-300 p-3  text-sm font-light text-gray-900 text-gray-900 focus:border-blue-500 focus:border-blue-500 focus:ring-cyan-200 focus:ring-cyan-200 dark:border-gray-600 dark:border-gray-600 dark:text-white dark:text-white dark:placeholder-gray-400 dark:placeholder-gray-400 dark:focus:border-blue-500 dark:focus:border-blue-500 dark:focus:ring-blue-500 dark:focus:ring-blue-500": true
      }
    },
    {
      ".radio-option": {
        "@apply ring-2 ring-gray-200 checked:ring-0 focus:ring-transparent text-blue-400": true
      }
    },
    {
      ".modal-backdrop.show": {
        "@apply z-10": true
      }
    },
    {
      ".product-box-primary": {
        "@apply pr-10": true
      },
      ".product-box-primary .btn-primary-outlined": {
        "@apply text-[12px] font-bold": true
      },
      ".product-box-primary .title": {
        "@apply text-right text-[14px] mb-5": true
      },
      ".product-box-primary .attributes": {
        "@apply text-right text-[12px] font-light text-gray-700 space-y-2": true
      },
      ".product-box-primary .btn-gray-outlined": {
        "@apply text-[12px] font-bold": true
      },
      ".product-box-primary .image": {
        "@apply p-1 w-1/6  grid grid-cols-1 pb-2": true
      },
      ".product-box-primary .image .counter-box-container": {
        "@apply place-self-end w-full flex justify-center": true
      },
      ".product-box-primary .image a": {
        "@apply justify-center flex flex-wrap": true
      },
      ".product-box-primary .image img": {
        "@apply max-w-full w-28": true
      },
      ".product-box-primary .delivery": {
        "@apply grow-0 text-right flex gap-3 w-1/2 font-light text-[14px]": true
      },
      ".product-box-primary .delivery .icon-gray": {
        "@apply ml-1": true
      }
    },
    {
      ".tabs-primary": {
        "@apply flex": true
      },
      ".tabs-primary li": {
        "@apply sm:grow lg:grow-0": true
      },
      ".tabs-primary button": {
        "@apply inline-block w-full font-semibold text-sm rounded-t-lg border-b-2 border-transparent py-4 px-4 text-center text-gray-500": true
      },
      ".tabs-primary [role='tab'].active": {
        "@apply border-b-red-600 font-extrabold border-b-4 rounded-r border-r-4 border-r-transparent  rounded-l border-l-4 border-l-transparent text-red-600 hover:border-red-600 hover:border-t-transparent hover:border-l-transparent hover:border-r-transparent hover:text-red-600": true
      }
    }
  ],
  "@tailwind components\n@layer components": {
    ".time-table .nav-tabs li a": {
      "@apply block font-medium text-xs leading-tight uppercase border-x-0 border-t-0 border-b-2 border-transparent px-6 p-0 my-0": true
    },
    ".time-table .nav-tabs li a.active": {
      "@apply text-blue-300 fill-blue-300 border-blue-300 border-0": true
    },
    ".time-table .nav-tabs li a.active p:nth-child(2)": {
      "@apply border-b-2 border-blue-300": true
    },
    ".time-table .nav-tabs li a div": {
      "@apply flex flex-col text-center h-20 justify-around items-center p-0 text-[16px] font-bold pt-2": true
    },
    ".time-table .nav-tabs li a div p": {
      "@apply pb-4": true
    },
    ".time-table .nav-tabs li a div p:nth-child(2)": {
      "@apply text-[18px] font-semibold text-gray-600  w-8": true
    },
    ".list-options input[type='radio'] + label": {
      "@apply fill-gray-400 cursor-pointer": true
    },
    ".list-options input[type='radio']:checked + label": {
      "@apply fill-blue-400": true
    }
  },
